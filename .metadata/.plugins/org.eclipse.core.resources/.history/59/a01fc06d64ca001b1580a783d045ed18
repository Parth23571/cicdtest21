package com.stc.gitcheck.cicdtest.scripts;

import org.testng.annotations.Test;

public class PlusMinus {
	public static int aa=0;
	@Test
  public void f() {
  System.out.println("Plus Minus");
 
  for(int intPlus=1;intPlus<=6;intPlus++)
  {
	 // aa=aa+1;
	  if (aa==6)
	  {
		  break;
	  }
	  System.out.print("+");
	  for(int intMinus=intPlus;intMinus <= intPlus;intMinus++)
	  {
		  System.out.print("-");
		 intMinus=intMinus+1;
		 aa=aa+1;
	  }
  }
  }
}
